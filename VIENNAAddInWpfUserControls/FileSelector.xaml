<UserControl x:Class="VIENNAAddInWpfUserControls.FileSelector"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Width="200" KeyboardNavigation.IsTabStop="True">
    <DockPanel Name="dockPanel">
        <Button KeyboardNavigation.TabIndex="1" DockPanel.Dock="Right" Margin="0,0,0,0" Name="openFileDialogButton" VerticalAlignment="Stretch" Padding="3,0" Click="openFileDialogButton_Click">...</Button>
        <TextBox KeyboardNavigation.TabIndex="0" Margin="0,0,0,0" Name="fileNameTextBox" VerticalAlignment="Stretch" TextChanged="fileNameTextBox_TextChanged" />
    </DockPanel>
</UserControl>
