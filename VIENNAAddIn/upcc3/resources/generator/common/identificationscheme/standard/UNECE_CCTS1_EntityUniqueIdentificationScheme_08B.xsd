<xsd:schema xmlns:ism6ccts1="urn:un:unece:uncefact:identificationscheme:standard:6:CCTS1:08B" xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:un:unece:uncefact:identificationscheme:standard:6:CCTS1:08B" elementFormDefault="qualified" attributeFormDefault="unqualified" version="3.0">
	<!-- ================================================================== -->
	<!-- =====  Element Declarations                                  ===== -->
	<!-- ================================================================== -->
	<!-- =====  Root element                                          ===== -->
	<!-- ================================================================== -->
	<xsd:element name="EntityUniqueIdentificationScheme" type="ism6ccts1:EntityUniqueIdentificationSchemeContentType"/>
	<!-- ================================================================== -->
	<!-- =====  Type Definitions                                      ===== -->
	<!-- ================================================================== -->
	<!-- =====  Type Definition:  Entity Unique Identification Scheme Content Type ===== -->
	<!-- ================================================================== -->
	<xsd:simpleType name="EntityUniqueIdentificationSchemeContentType">
		<xsd:restriction base="xsd:token">
			<xsd:length value="10"/>
			<xsd:pattern value="UNBE[0-9]{6}"/>
		</xsd:restriction>
	</xsd:simpleType>
</xsd:schema>
